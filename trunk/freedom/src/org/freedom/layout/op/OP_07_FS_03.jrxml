FROM PPOP O, EQPRODUTO P, PPOPFASE F,ppop OP,
ppfase fa, ppestrufase ef, ppopfase opf, pprecurso re
WHERE O.CODEMPPD=P.CODEMP AND O.CODFILIALPD=P.CODFILIAL AND O.CODPROD=P.CODPROD
