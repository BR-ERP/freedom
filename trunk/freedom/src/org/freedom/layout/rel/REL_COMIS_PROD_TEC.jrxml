	<queryString><![CDATA[select
pd.codsecao, se.descsecao, pd.precocomisprod, rc.perccomisgeral/100 perccomisgeral,  ir.perccomisitrc/100 perccomisitrc, 
sum((rc.perccomisgeral/100) * (ir.perccomisitrc/100)) perccomisfinal, sum((op.qtdfinalprodop * pd.precocomisprod)) vlrfinal,
vd.nomevend, sum(op.qtdfinalprodop) qtdfinalprodop

from ppop op

inner join eqproduto pd on
pd.codemp=op.codemp and pd.codfilial=op.codfilial and pd.codprod=op.codprod

inner join vdregracomis rc on
rc.codempsc=pd.codempsc and rc.codfilialsc=pd.codfilialsc and rc.codsecao=pd.codsecao

inner join eqsecao se on
se.codemp=pd.codempsc and se.codfilial=pd.codfilialsc and se.codsecao=pd.codsecao

left outer join eqrecmerc rm on
rm.codemp=op.codempos and rm.codfilial=op.codfilialos and rm.ticket=op.ticket

left outer join vdcliente cl on
cl.codemp=rm.codempcl and cl.codfilial=rm.codfilialcl and cl.codcli=rm.codcli

left outer join eqitrecmerc it on
it.codemp=op.codempos and it.codfilial=op.codfilialos and it.ticket=op.ticket and it.coditrecmerc=op.coditrecmerc

left outer join vditregracomis ir on
ir.codemp=rc.codemp and ir.codfilial=rc.codfilial and ir.codregrcomis=rc.codregrcomis

left outer join vdvendedor vd on
vd.codemp=ir.codempvd and vd.codfilial=ir.codfilialvd and vd.codvend=ir.codvend

where

op.dtfabrop between '2010-01-01' and '2011-01-01'
and op.sitop='FN'

group by 1,2,3,4,5,6

order by pd.codsecao]]></queryString>

