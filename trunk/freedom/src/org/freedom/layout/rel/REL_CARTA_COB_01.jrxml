	<queryString><![CDATA[select
(CAST('TODAY' AS DATE) - ir.dtvencitrec) diaAtraso,
ir.dtvencitrec vencimento, cl.codcli, cl.razcli, t.TIPOTBJ,
rc.datarec emissao, ir.dtpagoitrec recebimento,
rc.docrec doc, ir.vlrparcitrec, ir.vlrpagoitrec recebido,
ir.vlrapagitrec aberto,
    case t.tipotbj
        when 'D' then (ir.vlrparcitrec *((CAST('TODAY' AS DATE) - ir.dtvencitrec) * it.percittbj)/ 100)
        when 'M' then (ir.vlrparcitrec *((CAST('TODAY' AS DATE) - ir.dtvencitrec) * it.percittbj)/ 100*30)
        when 'B' then (ir.vlrparcitrec *((CAST('TODAY' AS DATE) - ir.dtvencitrec) * it.percittbj)/ 100*60)
        when 'T' then (ir.vlrparcitrec *((CAST('TODAY' AS DATE) - ir.dtvencitrec) * it.percittbj)/ 100*90)
        when 'S' then (ir.vlrparcitrec *((CAST('TODAY' AS DATE) - ir.dtvencitrec) * it.percittbj)/ 100*182)
        when 'A' then (ir.vlrparcitrec *((CAST('TODAY' AS DATE) - ir.dtvencitrec) * it.percittbj)/ 100*365)
    end
from fnitreceber ir, fnreceber rc, vdcliente cl, fntbjuros t, FNITTBJUROS it , sgprefere1 sg
where
ir.codemp=rc.codemp and ir.codfilial=rc.codfilial and ir.codrec=rc.codrec
and cl.codemp=rc.codempcl and cl.codfilial=rc.codfilialcl and cl.codcli=rc.codcli
and ir.statusitrec not in('RP') and rc.codempcl=71 and rc.codfilialcl=1 and rc.codcli=1
and t.codemp = it.codemp and t.codfilial = it.codfilial and t.codtbj = it.codtbj
and t.codemp = ir.codemp and t.codfilial = ir.codfilial
and it.codemp = ir.codemp and it.codfilial = ir.codfilial
and t.codemp = sg.codemptj and t.codfilial = sg.codfilialtj and t.codtbj = sg.codtbj
order by 1, 3 desc]]></queryString>

