	<queryString>
		<![CDATA[select
    CAST('S' AS VARCHAR(1)) AS tipo,
    vd.codcli as codpessoa,
    coalesce (vc.nomecli, vc.razcli) as nomepessoa,
    VD.CODVENDA as codlanc,
    vd.dtemitvenda dtemit,
    vd.vlrliqvenda as vlrliq,
    vd.vlrprodvenda as vlrtot,
    sum(coalesce(lfi.vlrbasepis,0)) vlrbasepis,
    sum(coalesce(lfi.vlrbasecofins,0)) vlrbasecofins,
    sum(coalesce(lfi.vlrpis,0)) vlrpis,
    sum(coalesce(lfi.vlrcofins,0)) vlrcofins
from vdvenda vd
left outer join  eqtipomov tm
    on tm.codemp=vd.codemptm
   and tm.codfilial=vd.codfilialtm
   and tm.codtipomov=vd.codtipomov
left outer join  vditvenda iv
    on iv.codemp=vd.codemp and iv.codfilial=vd.codfilial
   and iv.tipovenda=vd.tipovenda
   and iv.codvenda=vd.codvenda
left outer join eqproduto pd
    on pd.codemp=iv.codemppd and pd.codfilial=iv.codfilialpd
   and pd.codprod=iv.codprod
left outer join lfitvenda lfi
    on lfi.codemp=iv.codemp
   and lfi.codfilial=iv.codfilial
   and lfi.codvenda=iv.codvenda
   and lfi.tipovenda=iv.tipovenda
   and lfi.coditvenda=iv.coditvenda
  left outer join vdcliente vc
    on vc.codcli = vd.codcli
   and vc.codemp = vd.codempcl
   and vc.codfilial = vd.codfilialcl
 where vd.codemp=49
   and vd.codfilial=1
--   and vd.dtemitvenda between ? AND ?
   and tm.fiscaltipomov='S'
   and vd.statusvenda <> 'X'
 group by 1,2,3, 4, 5, 6, 7

 union all

 select
    CAST('E' AS VARCHAR(1)) AS tipo,
    cp.codfor as codpessoa,
    cf.nomefor as nomepessoa,
    cp.codcompra as codlanc,
    cp.dtemitcompra dtemit,
    cp.vlrliqcompra as vlrliq,
    cp.vlrprodcompra as vlrtot,
    sum(coalesce(lfi.vlrbasepis,0)) vlrbasepis,
    sum(coalesce(lfi.vlrbasecofins,0)) vlrbasecofins,
    sum(coalesce(lfi.vlrpis,0)) vlrpis,
    sum(coalesce(lfi.vlrcofins,0)*-1) vlrcofins
  from cpcompra cp
  left outer join  eqtipomov tm
    on tm.codemp=cp.codemptm
   and tm.codfilial=cp.codfilialtm
   and tm.codtipomov=cp.codtipomov
  left outer join  cpitcompra ic
    on ic.codemp=cp.codemp
   and ic.codfilial=cp.codfilial
   and ic.codcompra=cp.codcompra
  left outer join eqproduto pd
    on pd.codemp=ic.codemppd
   and pd.codfilial=ic.codfilialpd
   and pd.codprod=ic.codprod
  left outer join lfitcompra lfi
    on lfi.codemp=ic.codemp
   and lfi.codfilial=ic.codfilial
   and lfi.codcompra=ic.codcompra
   and lfi.coditcompra=ic.coditcompra
  left outer join cpforneced cf
    on cf.codfor = cp.codfor
   and cf.codemp = cp.codempfr
   and cf.codfilial = cp.codfilialfr
 where cp.codemp=49
   and cp.codfilial=1
--   and cp.dtemitcompra between ? AND ?
   and tm.fiscaltipomov='S'
   and cp.statuscompra <> 'X'
 group by 1,2, 3, 4, 5, 6, 7]]>
