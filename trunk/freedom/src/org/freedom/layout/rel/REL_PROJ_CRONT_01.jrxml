	<queryString>
		<![CDATA[SELECT
	CT.DESCCONTR,
	CT.CODCONTR,
	CT.CODCLI,
	CL.RAZCLI,
	SUM(I.QTDITCONTR) QTDITCONTR,
	SUM(I.VLRITCONTR) VLRITCONTR,
	SUM(I.QTDITCONTR*I.VLRITCONTR) TOTITCONTR
FROM
	VDCONTRATO CT,
	VDITCONTRATO I,
	VDCLIENTE CL
WHERE
	I.CODEMP=CT.CODEMP AND
	I.CODFILIAL=CT.CODFILIAL AND
	I.CODCONTR=CT.CODCONTR AND
	CT.CODEMP=3 AND
	CT.CODFILIAL=1 AND
	CL.CODEMP=CT.CODEMPCL AND
	CL.CODFILIAL=CT.CODFILIALCL AND
	CL.CODCLI=CT.CODCLI
GROUP BY
	CT.DESCCONTR,
	CT.CODCONTR,
	CT.CODCLI,
	CL.RAZCLI
ORDER BY
	CL.RAZCLI,
	CT.CODCONTR,
	CT.DESCCONTR,
	CT.CODCONTR]]>
	</queryString>
