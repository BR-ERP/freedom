	<queryString>
		<![CDATA[SELECT IT.DTITREC, IT.DTVENCITREC,IT.NPARCITREC,R.CODVENDA,R.CODCLI,C.RAZCLI,
C.NOMECLI, C.DDDCLI, C.FONECLI, IT.VLRPARCITREC, IT.VLRPAGOITREC,
IT.VLRAPAGITREC, IT.DTPAGOITREC,
R.DOCREC, IT.OBSITREC, (SELECT V.STATUSVENDA FROM VDVENDA V
WHERE V.FLAG IN ('S','N') AND V.CODEMP=R.CODEMPVA AND
V.CODFILIAL=R.CODFILIALVA AND V.CODVENDA=R.CODVENDA AND
V.TIPOVENDA=R.TIPOVENDA) FROM FNRECEBER R,VDCLIENTE C  ,FNITRECEBER IT
WHERE R.FLAG IN ('S','N')AND R.CODEMP=4 AND R.CODFILIAL=1 AND
IT.DTVENCITREC BETWEEN '01.01.2010' AND '31.01.2010' AND
IT.STATUSITREC IN ('R1') AND R.CODREC = IT.CODREC AND IT.CODEMP=R.CODEMP AND
IT.CODFILIAL=R.CODFILIAL AND C.CODEMP = R.CODEMPCL AND C.CODFILIAL=R.CODFILIALCL AND
C.CODCLI=R.CODCLI  ORDER BY IT.DTVENCITREC ,C.RAZCLI]]>
