	<queryString>
		<![CDATA[select 'R' tipo, 'V' tiporecdesp,  vd.dtemitvenda data , vc.codcontr
, cl.codcli, cl.razcli , vc.coditcontr, ct.desccontr
, ic.descitcontr
, cast('Faturamento/venda - pedido: '||vd.codvenda||' - serie/doc: '||vd.serie||'/'||vd.docvenda as varchar(200)) descricao
, iv.qtditvenda qtdade, cast((iv.vlrliqitvenda / case when iv.qtditvenda=0 then 1 else iv.qtditvenda end) as decimal(15,5)) vlrunit
, iv.vlrliqitvenda as vlrtotal from vditvendavditcontr vc, vditvenda iv, vdvenda vd , vdcontrato ct
, vditcontrato ic, vdcliente cl where ct.codemp=vc.codempct and ct.codfilial=vc.codfilialct
and ct.codcontr=vc.codcontr and ic.codemp=ct.codemp and ic.codfilial=ct.codfilial
and ic.codcontr=ct.codcontr and ic.coditcontr=vc.coditcontr and cl.codemp=ct.codempcl
and cl.codfilial=ct.codfilialcl and cl.codcli=ct.codcli
and vc.codempct=104 and vc.codfilialct=1 and vc.codcontr=7
 and iv.codemp=vc.codemp and iv.codfilial=vc.codfilial and iv.tipovenda=vc.tipovenda
 and iv.codvenda=vc.codvenda and iv.coditvenda=vc.coditvenda and vd.codemp=iv.codemp
 and vd.codfilial=iv.codfilial and vd.tipovenda=iv.tipovenda and vd.codvenda=iv.codvenda
 and iv.qtditvenda>0 union all select 'D' tipo, cp.tipocusto tiporecdesp,  cp.data
 , cp.codcontr, cl.codcli, cl.razcli , cp.coditcontr, ct.desccontr, ic.descitcontr
 , cp.desccusto descricao , cp.qtdcusto qtdade, cp.vlrcusto vlrunit
 , (cp.qtdcusto * cp.vlrcusto) as vlrtotal
 from vwcustoproj01 cp, vdcontrato ct, vditcontrato ic, vdcliente cl
 where ct.codemp=cp.codemp and ct.codfilial=cp.codfilial and ct.codcontr=cp.codcontr
 and ic.codemp=ct.codemp and ic.codfilial=ct.codfilial and ic.codcontr=ct.codcontr
 and ic.coditcontr=cp.coditcontr and cl.codemp=ct.codempcl and cl.codfilial=ct.codfilialcl
 and cl.codcli=ct.codcli and cp.codemp=104 and cp.codfilial=7
  and cp.codcontr=7 order by 5, 4, 7, 1 desc, 2, 3]]>
