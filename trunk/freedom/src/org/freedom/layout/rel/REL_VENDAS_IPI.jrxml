                FROM VDVENDA V,EQTIPOMOV T
                WHERE V.DTEMITVENDA BETWEEN '2010-01-10' AND '2010-04-30' AND
                V.CODTIPOMOV=T.CODTIPOMOV AND T.FISCALTIPOMOV='S'
                AND (NOT SUBSTR(V.STATUSVENDA,1,1)='C') GROUP BY V.DTEMITVENDA,T.ESTIPOMOV
