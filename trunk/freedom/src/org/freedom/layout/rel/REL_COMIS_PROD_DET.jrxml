	<queryString><![CDATA[select

    sc.codsecao, sc.descsecao,
    rc.perccomisgeral/100 perccomisgeral,
    rm.dtent, rm.ticket, rm.docrecmerc,
    ir.qtditrecmerc, ir.garantia,
    pd.refprod, pd.descprod, pd.precocomisprod, pd.codprod

from

    eqrecmerc rm

    left outer join eqitrecmerc ir on
        ir.codemp=rm.codemp and ir.codfilial=rm.codfilial and ir.ticket=rm.ticket

    left outer join eqproduto pd on
        pd.codemp=ir.codemppd and pd.codfilial=ir.codfilialpd and pd.codprod=ir.codprod

    left outer join eqsecao sc on
        sc.codemp=pd.codempsc and sc.codfilial=pd.codfilialsc and sc.codsecao=pd.codsecao

    inner join vdregracomis rc on
        rc.codempsc=sc.codemp and rc.codfilialsc=sc.codfilial and rc.codsecao=sc.codsecao

where

    rm.dtent between '2010-01-01' and '2011-01-01' --and ir.statusitrecmerc = 'FN'


order by sc.codsecao, rm.dtent]]></queryString>

