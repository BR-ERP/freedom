	<queryString>
		<![CDATA[select a.anoatendo, a.mesatendo, a.nomeatend,
c.codcontr, c.desccontr, ic.coditcontr, ic.descitcontr,
sum(a.totalgeral) totalgeral,
sum(a.totalmeta) totalmeta, sum(a.totalcomis) totalcomis,
sum(a.totalcobcli) totalcobcli
from atatendimentovw02 a
left outer join vdcontrato c on
	c.codemp=a.codempct and c.codfilial=a.codfilialct and c.codcontr=a.codcontr
left outer join vditcontrato ic on
	ic.codemp=c.codemp and ic.codfilial=c.codfilial and ic.codcontr=c.codcontr
where
a.codemp=3 and a.codfilial=1 and a.dataatendo between '01.10.2011' and '30.10.2011'
group by a.anoatendo, a.mesatendo, a.nomeatend,
c.codcontr, c.desccontr, ic.coditcontr, ic.descitcontr
order by a.anoatendo, a.mesatendo, a.nomeatend,
c.desccontr, ic.descitcontr ]]>
