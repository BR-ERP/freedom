	<queryString><![CDATA[SELECT SUM(P.VLRLIQPED) AS VALOR,
( CASE EXTRACT(MONTH FROM P.DATAPED)
       WHEN 1 THEN 'Janeiro'
       WHEN 2 THEN 'Fevereiro'
       WHEN 3 THEN 'Março'
       WHEN 4 THEN 'Abril'
       WHEN 5 THEN 'Maio'
       WHEN 6 THEN 'Junho'
       WHEN 7 THEN 'Julho'
       WHEN 8 THEN 'Agosto'
       WHEN 9 THEN 'Setembro'
       WHEN 10 THEN 'Outubro'
       WHEN 11 THEN 'Novembro'
       WHEN 12 THEN 'Dezembro'
   END
) AS MES
FROM RPPEDIDO P
WHERE P.CODEMP=$P{CODEMP} AND P.CODFILIAL=$P{CODFILIAL}
GROUP BY 2]]></queryString>

