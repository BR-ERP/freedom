	<queryString>
		<![CDATA[SELECT SUM(P.VLRLIQPED) AS VALOR,
( CASE EXTRACT(MONTH FROM P.DATAPED)
       WHEN 0 THEN 'Janeiro'
       WHEN 1 THEN 'Fevereiro'
       WHEN 2 THEN 'Março'
       WHEN 3 THEN 'Abril'
       WHEN 4 THEN 'Maio'
       WHEN 5 THEN 'Junho'
       WHEN 6 THEN 'Julho'
       WHEN 7 THEN 'Agosto'
       WHEN 8 THEN 'Setembro'
       WHEN 9 THEN 'Outubro'
       WHEN 10 THEN 'Novembro'
       WHEN 11 THEN 'Dezembro'
   END
) AS MES,
EXTRACT(MONTH FROM P.DATAPED) AS IMES
FROM RPPEDIDO P
WHERE P.CODEMP=$P{CODEMP} AND P.CODFILIAL=$P{CODFILIAL}
GROUP BY 2,3]]>
	</queryString>
