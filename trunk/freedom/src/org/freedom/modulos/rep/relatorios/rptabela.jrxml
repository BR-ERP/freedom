	<queryString>
		<![CDATA[select pr.refprod,pr.descprod,pr.saldoprod as compra,sum(it.qtditped) venda,
pr.saldoprod-sum(it.qtditped) as saldo, pr.precoprod1
from rpproduto pr, rpitpedido it, rppedido pd
where pr.codemp=it.codemppd and pr.codfilial=it.codfilialpd and pr.codprod = it.codprod
and pd.codemp = it.codemp and pd.codfilial = it.codfilial and pd.codped = it.codped
and pd.dataped between '2008-01-01' and '2008-05-01'
and pd.codfor = 1
group by 1,2,3,6]]>
