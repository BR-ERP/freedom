	<queryString><![CDATA[SELECT C.RAZCLI, V.CODCLI, P.DESCPROD, IV.CODPROD,
MAX(V.DTEMITVENDA) DTEMITVENDA, MAX(V.DOCVENDA) DOCVENDA,
MAX(V.SERIE) SERIE,
MAX (IV.VLRLIQITVENDA/(CASE WHEN IV.QTDITVENDA=0 THEN 1 ELSE IV.QTDITVENDA END)) PRECOVENDA
FROM VDCLIENTE C, VDVENDA V, VDITVENDA IV, EQPRODUTO P
WHERE C.CODEMP=V.CODEMPCL AND C.CODFILIAL=V.CODFILIALCL AND
C.CODCLI=V.CODCLI AND C.CODEMP=$P{CODEMP} AND C.CODFILIAL=$P{CODFILIAL} AND
IV.CODEMP=V.CODEMP AND IV.CODFILIAL=V.CODFILIAL AND
IV.TIPOVENDA=V.TIPOVENDA AND IV.CODVENDA=V.CODVENDA AND
P.CODEMP=IV.CODEMPPD AND P.CODFILIAL=IV.CODFILIALPD AND
P.CODPROD=IV.CODPROD AND
V.DTEMITVENDA BETWEEN '01.01.2000' AND '31.12.2008'
GROUP BY C.RAZCLI, V.CODCLI, P.DESCPROD, IV.CODPROD]]></queryString>

