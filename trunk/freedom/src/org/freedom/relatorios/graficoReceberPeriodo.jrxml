		<queryString><![CDATA[SELECT
  SUM( IR.VLRITREC ) AS VALOR,
  ((IR.DTVENCITREC-IR.DTITREC)/30)*30 AS PERIODO
FROM
  FNITRECEBER IR
WHERE
  IR.CODEMP=$P{CODEMP} AND IR.CODFILIAL=$P{CODFILIAL} AND
  IR.DTITREC BETWEEN $P{DTINI} AND $P{DTFIM}
GROUP BY
  2
ORDER BY
  2]]></queryString>

