	<queryString>
		<![CDATA[select p.codemp, p.codfilial, p.codprod, p.refprod, p.descprod, p.sldliqprod
, (select first 1 dtmovprod from eqmovprod mp
  where mp.codemp=71 and mp.codfilial=1
  and mp.codemppd=p.codemp and mp.codfilialpd=p.codfilial
  and mp.codprod=p.codprod and mp.tipomovprod='E' and mp.codcompra is not null
  order by mp.dtmovprod desc, mp.codmovprod desc
) dtultcpprod
, (select first 1 qtdmovprod from eqmovprod mp
  where mp.codemp=71 and mp.codfilial=1
  and mp.codemppd=p.codemp and mp.codfilialpd=p.codfilial
  and mp.codprod=p.codprod and mp.tipomovprod='E' and mp.codcompra is not null
  order by mp.dtmovprod desc, mp.codmovprod desc
) qtdultcpprod
, sum((case when extract(month from v.dtemitvenda)=01
and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd1
, sum((case when extract(month from v.dtemitvenda)=02 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd2
, sum((case when extract(month from v.dtemitvenda)=03 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd3
, sum((case when extract(month from v.dtemitvenda)=04 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd4
, sum((case when extract(month from v.dtemitvenda)=05 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd5
, sum((case when extract(month from v.dtemitvenda)=06 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd6
, sum((case when extract(month from v.dtemitvenda)=07 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd7
, sum((case when extract(month from v.dtemitvenda)=08 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd8
, sum((case when extract(month from v.dtemitvenda)=09 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd9
, sum((case when extract(month from v.dtemitvenda)=10 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd10
, sum((case when extract(month from v.dtemitvenda)=11 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd11
, sum((case when extract(month from v.dtemitvenda)=12 and extract(year from v.dtemitvenda)=2013 then iv.qtditvenda else 0 end)) qtd12
, sum(iv.qtditvenda) subtotal
, sum(iv.qtditvenda) / 11 qtdmedia  from eqproduto p
 inner join vditvenda iv on  iv.codemppd=p.codemp and iv.codfilialpd=p.codfilial and iv.codprod=p.codprod
 inner join vdvenda v on  v.codemp=iv.codemp and v.codfilial=iv.codfilial and v.tipovenda=iv.tipovenda and v.codvenda=iv.codvenda
  inner join eqtipomov tm on  tm.codemp=v.codemptm and tm.codfilial=v.codfilialtm and tm.codtipomov=v.codtipomov
  inner join fnplanopag pp on  pp.codemp=v.codemppg and pp.codfilial=v.codfilialpg and pp.codplanopag=v.codplanopag
  where p.codemp=71 and p.codfilial=1  and v.codemp=71 and v.codfilial=1
  and v.dtemitvenda between '01.01.2013' and '31.12.2013'
  and v.flag in ('S','N') and tm.fiscaltipomov='S'
  and tm.somavdtipomov='S' and pp.parcplanopag>0
  and substring(v.statusvenda from 1 for 1) not in ('C','N')
  group by 1,2,3,4,5,6
   order by p.descprod, p.codprod]]>
	</queryString>
