		<![CDATA[select rtrim(coalesce(u.pnomeusu,'')) || ' ' || rtrim(coalesce(u.unomeusu,'')) as usuario, a.dtainiagd,a.hriniagd,a.dtafimagd,a.hrfimagd, a.assuntoagd, a.descagd, a.diatodo
from sgagenda a, sgusuario u
where
a.codemp=3 and a.codfilial=1 and a.codage=2
and u.codempae=a.codempae and u.codfilialae=a.codfilialae and u.codage=a.codage
ORDER BY A.DTAINIAGD DESC,A.HRINIAGD DESC,A.DTAFIMAGD DESC,A.HRFIMAGD DESC]]>
