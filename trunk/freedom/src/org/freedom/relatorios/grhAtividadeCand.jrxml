from rhcandidato ca, rhcandidatostatus cs left outer join rhvaga vg on
vg.codemp = cs.codempvg and vg.codfilial=cs.codfilialvg and vg.codvaga=cs.codvaga
left outer join rhempregador em on
em.codemp=vg.codempem and em.codfilial=vg.codfilialem and em.codempr=vg.codempr
left outer join rhfuncao fc on
fc.codemp = vg.codempfc and fc.codfilial = vg.codfilialfc and fc.codfunc = vg.codfunc
where ca.codemp = cs.codemp and ca.codfilial=cs.codfilial and ca.codcand=cs.codcand
and cs.dtstatus between $P{DATAINI} and $P{DATAFIM} and ca.codemp=$P{CODEMP} 
and ca.codfilial=$P{CODFILIAL}
order by cs.dtstatus]]></queryString>

