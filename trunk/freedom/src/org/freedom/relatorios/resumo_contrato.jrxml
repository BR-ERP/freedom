a.vlritcontr valor, a.vlritcontrexced valorexcedente, a.dtinicio,
sum(a.totalcobcli) qtdhoras
from atatendimentovw02 a
where a.codempct=$P{CODEMP} and a.codfilialct=$P{CODFILIAL} and a.codcli=$P{CODCLI} and a.codcontr=$P{CODCONTR} and a.coditcontr=$P{CODITCONTR} and
a.dataatendo between $P{DTINIAC} and
