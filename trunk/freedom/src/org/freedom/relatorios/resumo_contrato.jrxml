	<queryString>
		<![CDATA[SELECT CT.codcontr, IC.qtditcontr,
sum(( (case when a.cobcliespec='S' and a.statusatendo<>'NC' then (case when a.totalmin<a.tempomincobespec
then a.tempomincobespec else
(case when a.totalmin>a.tempomaxcobespec and a.tempomaxcobespec<>0
then a.tempomaxcobespec else a.totalmin end) end)  else 0 end)
))/60 qtdhoras,
IC.vlritcontr VALOR, IC.vlritcontrexced VALOREXCEDENTE
FROM VDITCONTRATO IC, VDCONTRATO CT, atatendimentovw01 A
WHERE IC.CODEMP = CT.CODEMP AND IC.CODFILIAL = CT.CODFILIAL AND IC.codcontr=CT.codcontr
AND A.codemp=$P{CODEMP} AND A.CODFILIAL=$P{CODFILIAL} AND A.codcli=$P{CODCLI} and A.codcontr=$P{CODCONTR}
AND CT.codempcl=A.codempcl AND CT.codfilialcl=A.codfilialcl AND CT.codcli=A.CODCLI
AND CT.codemp=A.codempct AND CT.CODFILIAL=A.CODFILIALCT AND CT.codcontr=A.codcontr
AND IC.coditcontr=A.coditcontr
and A.dataatendo between $P{DTINI} and $P{DTFIM}
and A.coditcontr=$P{CODITCONTR}
group by 1,2,4,5]]>
	</queryString>
