	<queryString>
		<![CDATA[SELECT
  SL.CODPLAN, PL.DESCPLAN,
  SL.DATASUBLANCA, SL.HISTSUBLANCA, L.DOCLANCA, SL.VLRSUBLANCA,
  CC.DESCCC, C.DESCCONTA
FROM FNSUBLANCA SL
LEFT OUTER JOIN FNPLANEJAMENTO PL ON PL.CODEMP=SL.CODEMPPN AND PL.CODFILIAL=SL.CODFILIALPN AND PL.CODPLAN=SL.CODPLAN
LEFT OUTER JOIN FNLANCA L ON L.CODEMP=SL.CODEMP AND L.CODFILIAL=SL.CODFILIAL AND L.CODLANCA=SL.CODLANCA
LEFT OUTER JOIN FNCC CC ON CC.CODEMP=SL.CODEMPCC AND CC.CODFILIAL=SL.CODFILIALCC AND CC.CODCC=SL.CODCC AND CC.ANOCC=SL.ANOCC
LEFT OUTER JOIN FNCONTA C ON PL.CODEMP=SL.CODEMPPN AND PL.CODFILIAL=SL.CODFILIALPN AND PL.CODPLAN=SL.CODPLAN AND
  C.CODEMPPN=PL.CODEMP AND C.CODFILIALPN=PL.CODFILIAL AND C.CODPLAN=PL.CODPLAN
WHERE
  SL.CODEMP=42 AND SL.CODFILIAL=1 AND SL.DATASUBLANCA BETWEEN '2008-05-05' AND '2008-05-10'
ORDER BY
  SL.CODPLAN, SL.DATASUBLANCA]]>
	</queryString>
