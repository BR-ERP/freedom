	<queryString><![CDATA[SELECT F.CODFOR CODEMIT, F.RAZFOR RAZEMIT, CAST( '2008-1-1' AS DATE) DATA, 'A' TIPO, 0 DOC, 0.00 VLRDEB, 
(COALESCE( ( SELECT SUM(P.VLRPAG) FROM FNPAGAR P WHERE P.CODEMP=25 AND P.CODFILIAL=1 AND P.CODEMPFR=F.CODEMP AND
 P.CODFILIALFR=F.CODFILIAL AND P.CODFOR=F.CODFOR AND 
P.DATAPAG < '2008-1-1' ),0) + COALESCE( ( SELECT SUM(L.VLRLANCA) FROM FNLANCA L WHERE L.CODEMPFR=F.CODEMP AND 
L.CODFILIALFR=F.CODFILIAL AND L.CODFOR=F.CODFOR AND L.CODEMP=25 AND L.CODFILIAL=1 
AND L.DATALANCA < '2008-1-1' ), 0) ) VLRCRED FROM CPFORNECED F WHERE F.CODEMP=25 AND 
F.CODFILIAL=1 AND F.CODFOR=1 AND ( EXISTS (SELECT * FROM FNPAGAR P2 WHERE 
P2.CODEMP=25 AND P2.CODFILIAL=1 AND P2.CODEMPFR=F.CODEMP AND 
P2.CODFILIALFR=F.CODFILIAL AND P2.CODFOR=F.CODFOR AND DATAPAG BETWEEN '2008-1-1' AND '2008-1-30' )
OR EXISTS (SELECT * FROM FNLANCA L2 WHERE F.CODEMP=L2.CODEMPFR AND F.CODFILIAL=L2.CODFILIALFR AND
F.CODFOR=L2.CODFOR AND L2.CODEMP=25 AND L2.CODFILIAL=1 AND L2.DATALANCA BETWEEN '2008-1-1' AND '2008-1-30') ) 
UNION SELECT P.CODFOR CODEMIT, F.RAZFOR RAZEMIT, P.DATAPAG DATA, 'C' TIPO, P.DOCPAG DOC, 
(P.VLRPARCPAG-P.VLRPAG)*-1 VLRDEB, P.VLRPAG VLRCRED FROM FNPAGAR P, CPFORNECED F WHERE 
F.CODEMP=P.CODEMPFR AND F.CODFILIAL=P.CODFILIALFR AND F.CODFOR=P.CODFOR AND P.CODEMP=25 
AND P.CODFILIAL=1 AND F.CODFOR=1 AND P.DATAPAG BETWEEN '2008-1-1' AND '2008-1-30' UNION SELECT L.CODFOR CODEMIT,
F.RAZFOR RAZEMIT, L.DATALANCA DATA, 'P' TIPO, P.DOCPAG DOC, L.VLRLANCA VLRDEB, 0.00 VLRCRED 
FROM FNLANCA L, CPFORNECED F, FNPAGAR P WHERE F.CODEMP=L.CODEMPFR AND F.CODFILIAL=L.CODFILIALFR AND 
P.CODEMP=L.CODEMPPG AND P.CODFILIAL=L.CODFILIALPG AND P.CODPAG=L.CODPAG AND F.CODFOR=L.CODFOR AND 
F.CODFOR=1 AND L.CODEMP=25 AND L.CODFILIAL=1 AND L.DATALANCA BETWEEN '2008-1-1' AND '2008-1-30' 
ORDER BY 1, 2, 3, 4, 5]]></queryString>

