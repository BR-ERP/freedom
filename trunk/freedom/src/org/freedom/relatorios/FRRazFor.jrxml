	<queryString><![CDATA[SELECT F.CODFOR CODEMIT, F.RAZFOR RAZEMIT,
CAST('01.01.2007' AS DATE) DATA, 'A' TIPO, 0 DOC,
(COALESCE( ( SELECT SUM(P.VLRPARCPAG)
FROM FNPAGAR P
WHERE P.CODEMP=25 AND P.CODFILIAL=1 AND
P.CODEMPFR=F.CODEMP AND P.CODFILIALFR=F.CODFILIAL AND
P.CODFOR=F.CODFOR AND
P.DATAPAG < '01.01.2007' ),0) +
COALESCE( ( SELECT SUM(L.VLRLANCA)
FROM FNLANCA L, FNPAGAR P
WHERE L.CODEMPPG=P.CODEMP AND L.CODFILIALPG=P.CODFILIAL AND
L.CODPAG=P.CODPAG AND P.CODEMPFR=F.CODEMP AND P.CODFILIALFR=F.CODFILIAL AND
P.CODFOR=F.CODFOR AND
P.CODEMP=25 AND P.CODFILIAL=1 AND
L.DATALANCA < '01.01.2007' ), 0)
) VALOR
FROM CPFORNECED F
WHERE F.CODEMP=25 AND F.CODFILIAL=1 AND F.CODFOR=1 AND
( EXISTS (SELECT * FROM FNPAGAR P2
WHERE P2.CODEMP=25 AND P2.CODFILIAL=1 AND
P2.CODEMPFR=F.CODEMP AND P2.CODFILIALFR=F.CODFILIAL AND
P2.CODFOR=F.CODFOR AND DATAPAG BETWEEN '01.01.2007' AND '31.12.2007' )
OR EXISTS (SELECT * FROM FNLANCA L2, FNPAGAR P2
WHERE
L2.CODEMPPG=P2.CODEMP AND L2.CODFILIALPG=P2.CODFILIAL AND
L2.CODPAG=P2.CODPAG AND F.CODEMP=P2.CODEMPFR AND
F.CODFILIAL=P2.CODFILIALFR AND F.CODFOR=P2.CODFOR AND
P2.CODEMP=25 AND P2.CODFILIAL=1 AND
L2.DATALANCA BETWEEN '01.01.2007' AND '31.12.2007'
) )
UNION
SELECT P.CODFOR CODEMIT, F.RAZFOR RAZEMIT,
P.DATAPAG DATA, 'C' TIPO, P.DOCPAG DOC, P.VLRPARCPAG VALOR
FROM FNPAGAR P, CPFORNECED F
WHERE F.CODEMP=P.CODEMPFR AND F.CODFILIAL=P.CODFILIALFR AND
F.CODFOR=P.CODFOR AND
P.CODEMP=25 AND P.CODFILIAL=1 AND F.CODFOR=1 AND
P.DATAPAG BETWEEN '01.01.2007' AND '31.12.2007'
UNION
SELECT P.CODFOR CODEMIT, F.RAZFOR RAZEMIT,
L.DATALANCA DATA, 'P' TIPO, P.DOCPAG DOC, L.VLRLANCA
FROM FNLANCA L, FNPAGAR P, CPFORNECED F
WHERE L.CODEMPPG=P.CODEMP AND L.CODFILIALPG=P.CODFILIAL AND
L.CODPAG=P.CODPAG AND F.CODEMP=P.CODEMPFR AND F.CODFILIAL=P.CODFILIALFR AND
F.CODFOR=P.CODFOR AND F.CODFOR=1 AND
P.CODEMP=25 AND P.CODFILIAL=1 AND
L.DATALANCA BETWEEN '01.01.2007' AND '31.12.2007'
ORDER BY 1, 2, 3, 4, 5]]></queryString>

