EXISTS (SELECT * FROM FNSUBLANCA SL, FNLANCA L
WHERE L.FLAG IN ('S','N') AND SL.CODPLAN LIKE RTRIM(P.CODPLAN)||'%' AND SL.CODLANCA=L.CODLANCA AND SL.DATASUBLANCA  BETWEEN '01.03.2011' AND '31.03.2011' AND SL.CODEMP=L.CODEMP AND SL.CODFILIAL=L.CODFILIAL  AND L.CODEMP=P.CODEMP AND L.CODFILIAL=1)
