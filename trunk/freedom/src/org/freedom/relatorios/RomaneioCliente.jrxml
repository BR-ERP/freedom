	<queryString>
		<![CDATA[SELECT R.DATAROMA, P.CODPROD, P.DESCPROD,P.CODUNID, R.CODROMA,
SUM(I.QTDITVENDA) QTDITVENDA ,
SUM(I.VLRLIQITVENDA/I.QTDITVENDA) VLRUNIT,
SUM(I.VLRLIQITVENDA) VLRTOTAL
FROM VDROMANEIO R, VDITROMANEIO IR,
VDVENDA V,VDITVENDA I,EQPRODUTO P
WHERE R.CODROMA=2 AND IR.CODROMA=R.CODROMA
AND V.CODVENDA=IR.CODVENDA AND I.CODVENDA=V.CODVENDA
AND P.CODPROD=I.CODPROD AND I.QTDITVENDA>0
GROUP BY R.DATAROMA, P.CODPROD, P.DESCPROD, P.CODUNID, R.CODROMA
ORDER BY P.CODPROD]]>
