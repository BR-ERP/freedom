	<queryString>
		<![CDATA[SELECT C.CODCLI CODEMIT, C.RAZCLI RAZEMIT,  CAST( ' 2011-12-25' AS DATE) DATA, 'A' TIPO, 'A' TIPOSUBLANCA, 0 DOC, (COALESCE( ( SELECT SUM(R.VLRPARCREC+VLRMULTAREC+VLRJUROSREC) FROM FNRECEBER R WHERE R.CODEMP=71 AND R.CODFILIAL=1 AND R.CODEMPCL=C.CODEMP AND R.CODFILIALCL=C.CODFILIAL AND R.CODCLI=C.CODCLI AND R.DATAREC < '21.12.2011' ),0) - COALESCE( ( SELECT SUM(SL.VLRSUBLANCA*-1) FROM FNSUBLANCA SL WHERE   SL.CODEMPCL=C.CODEMP AND SL.CODFILIALCL=C.CODFILIAL AND SL.CODCLI=C.CODCLI AND  SL.TIPOSUBLANCA='P' AND  SL.CODEMP=71 AND SL.CODFILIAL=1 AND SL.DATASUBLANCA < '21.12.2011' ), 0) - ( COALESCE( ( SELECT SUM(CP.VLRLIQCOMPRA) FROM CPCOMPRA CP, EQTIPOMOV TM, EQCLIFOR CF WHERE CP.CODEMP=71 AND CP.CODFILIAL=1 AND  TM.CODEMP=CP.CODEMPTM AND TM.CODFILIAL = CP.CODFILIALTM AND TM.ESTIPOMOV='E' AND TM.TIPOMOV='DV' AND TM.CODTIPOMOV=CP.CODTIPOMOV AND CF.CODEMP=C.CODEMP AND CF.CODFILIAL=C.CODFILIAL AND CF.CODCLI=C.CODCLI AND CP.CODEMPFR=CF.CODEMPFR AND CP.CODFILIALFR=CF.CODFILIALFR AND CP.CODFOR=CF.CODFOR AND CP.DTENTCOMPRA < '21.12.2011' ),0) ) ) VLRDEB, 0.00 VLRCRED FROM VDCLIENTE C WHERE C.CODEMP=71 AND C.CODFILIAL=1 AND C.CODCLI=1050 AND ( EXISTS (SELECT * FROM FNRECEBER R2 WHERE R2.CODEMP=71 AND R2.CODFILIAL=1 AND R2.CODEMPCL=C.CODEMP AND R2.CODFILIALCL=C.CODFILIAL AND R2.CODCLI=C.CODCLI AND  DATAREC BETWEEN '21.12.2011' AND '25.12.2011' ) OR EXISTS (SELECT * FROM FNSUBLANCA SL2, FNRECEBER R2 WHERE SL2.CODEMPRC=R2.CODEMP AND SL2.CODFILIALRC=R2.CODFILIAL AND SL2.CODREC=R2.CODREC AND C.CODEMP=R2.CODEMPCL AND C.CODFILIAL=R2.CODFILIALCL AND C.CODCLI=R2.CODCLI AND R2.CODEMP=71 AND R2.CODFILIAL=1 AND SL2.DATASUBLANCA BETWEEN '21.12.2011' AND '25.12.2011') OR  EXISTS (SELECT * FROM CPCOMPRA CP, EQTIPOMOV TM, EQCLIFOR CF WHERE CP.CODEMP=71 AND CP.CODFILIAL=1 AND  TM.CODEMP=CP.CODEMPTM AND TM.CODFILIAL = CP.CODFILIALTM AND TM.CODTIPOMOV=CP.CODTIPOMOV AND TM.ESTIPOMOV='E' AND TM.TIPOMOV='DV' AND CF.CODEMP=C.CODEMP AND CF.CODFILIAL=C.CODFILIAL AND CF.CODCLI=C.CODCLI AND CP.CODEMPFR=CF.CODEMPFR AND CP.CODFILIALFR=CF.CODFILIALFR AND CP.CODFOR=CF.CODFOR AND CP.DTENTCOMPRA BETWEEN '21.12.2011' AND '25.12.2011' ) ) UNION ALL SELECT R.CODCLI CODEMIT, C.RAZCLI RAZEMIT, R.DATAREC DATA, 'Q' TIPO, 'V' TIPOSUBLANCA, R.DOCREC DOC, R.VLRPARCREC VLRDEB, (R.VLRMULTAREC-R.VLRJUROSREC)*-1 VLRCRED FROM FNRECEBER R, VDCLIENTE C WHERE C.CODEMP=R.CODEMPCL AND C.CODFILIAL=R.CODFILIALCL AND C.CODCLI=R.CODCLI AND R.CODEMP=71 AND R.CODFILIAL=1 AND C.CODCLI=1050 AND R.DATAREC BETWEEN '21.12.2011' AND '25.12.2011' UNION ALL SELECT R.CODCLI CODEMIT, C.RAZCLI RAZEMIT, SL.DATASUBLANCA DATA,  (CASE WHEN SL.TIPOSUBLANCA='P' THEN 'R' ELSE 'X' END) TIPO, SL.TIPOSUBLANCA, R.DOCREC DOC, (CASE WHEN SL.TIPOSUBLANCA IN ('J','D','M') THEN SL.VLRSUBLANCA*-1 ELSE 0.00 END) VLRDEB, SL.VLRSUBLANCA VLRCRED FROM FNSUBLANCA SL, FNRECEBER R, VDCLIENTE C WHERE SL.CODEMPRC=R.CODEMP AND SL.CODFILIALRC=R.CODFILIAL AND SL.CODREC=R.CODREC AND C.CODEMP=R.CODEMPCL AND C.CODFILIAL=R.CODFILIALCL AND C.CODCLI=R.CODCLI AND C.CODCLI=1050 AND R.CODEMP=71 AND R.CODFILIAL=1 AND SL.DATASUBLANCA BETWEEN '21.12.2011' AND '25.12.2011' UNION ALL SELECT C.CODCLI CODEMIT, C.RAZCLI RAZEMIT, CP.DTENTCOMPRA DATA,  'Z' TIPO, 'Z' TIPOSUBLANCA, CP.DOCCOMPRA DOC, 0.00 VLRDEB, (CP.VLRLIQCOMPRA * -1) VLRCRED FROM CPCOMPRA CP, EQTIPOMOV TM, EQCLIFOR CF, VDCLIENTE C WHERE TM.CODEMP=CP.CODEMPTM AND TM.CODFILIAL=CP.CODFILIALTM AND TM.CODTIPOMOV=CP.CODTIPOMOV AND TM.ESTIPOMOV='E' AND TM.TIPOMOV='DV' AND CF.CODEMP=C.CODEMP AND CF.CODFILIAL=C.CODFILIAL AND CF.CODCLI=C.CODCLI AND CP.CODEMPFR=CF.CODEMPFR AND CP.CODFILIALFR=CF.CODFILIALFR AND CP.CODFOR=CF.CODFOR AND C.CODCLI=1050 AND CP.CODEMP=71 AND CP.CODFILIAL=1 AND CP.DTENTCOMPRA BETWEEN '21.12.2011' AND '25.12.2011' ORDER BY 1, 2, 3, 4, 6, 5]]>
	</queryString>
