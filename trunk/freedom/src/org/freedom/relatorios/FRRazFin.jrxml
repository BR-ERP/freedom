	<queryString>
		<![CDATA[select sl.datasublanca, sl.codlanca, sl.histsublanca, sl.vlrsublanca , coalesce(f.codfor, c.codcli) codemit, coalesce(f.razfor, c.razcli) razemit , (case when f.codfor is not null then 'F' when c.codcli is not null then 'C' else 'A' end) tipo from fnsublanca sl left outer join cpforneced f on f.codemp=sl.codempfr and f.codfilial=sl.codfilialfr and f.codfor=sl.codfor left outer join vdcliente c on c.codemp=sl.codempcl and c.codfilial=sl.codfilialcl and c.codcli=sl.codcli where sl.codemp=$P{CODEMP} and sl.codfilial=$P{CODFILIAL} and sl.codplan=$P{CODPLAN} and sl.codemppn=$P{CODEMPPN} and sl.codfilialpn=$P{CODFILIALPN} and sl.datasublanca between $P{DTINI} and $P{DTFIM} order by sl.datasublanca]]>
