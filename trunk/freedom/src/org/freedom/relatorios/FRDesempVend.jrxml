	<queryString>
		<![CDATA[SELECT V.CODVEND, V.NOMEVEND, COUNT(DISTINCT VD.CODVENDA) AS TOTVENDCOMIS,
 SUM(VI.VLRLIQITVENDA) AS TOTVENDAS, (SUM(VI.VLRLIQITVENDA)/COUNT(DISTINCT VD.CODVENDA)) AS VLRMEDVENDA,
 COUNT(VI.CODITVENDA) AS QTDITENS, (SUM(VI.VLRLIQITVENDA)/COUNT(VI.CODITVENDA)) AS ITEMMEDIO
 , (SELECT COUNT(*) FROM VDORCAMENTO WHERE CODEMP=83 AND CODFILIAL=1 AND CODVEND=V.codvend  AND DTORC BETWEEN '01.02.2013' AND '13.05.2013') QTDORC


FROM VDVENDA VD
    LEFT OUTER JOIN VDVENDEDOR V ON V.CODVEND = VD.CODVEND LEFT OUTER JOIN VDITVENDA VI ON VI.CODVENDA = VD.CODVENDA
    LEFT OUTER JOIN EQTIPOMOV TM ON VD.CODEMPTM=TM.CODEMP AND VD.CODFILIALTM=TM.CODFILIAL AND VD.CODTIPOMOV=TM.CODTIPOMOV


WHERE V.CODEMP=83 AND V.CODFILIAL=1 AND NOT SUBSTR(VD.STATUSVENDA,1,1)='C' AND VD.DTEMITVENDA
BETWEEN '01.02.2013' AND '13.05.2013'  AND TM.FISCALTIPOMOV='S'  AND TM.SOMAVDTIPOMOV='S'  GROUP BY 1,2]]>
	</queryString>
