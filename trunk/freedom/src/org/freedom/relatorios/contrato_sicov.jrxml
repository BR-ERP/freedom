	<queryString>
		<![CDATA[select ct.codcontr, ct.dtinicio, ct.dtfim, ct.codcli, cl.razcli, cl.cpfcli, cl.agenciacli, cl.NContaBcoCli
from vdcontrato ct
inner join vdcliente cl on cl.codemp=ct.codempcl and cl.codfilial=ct.codfilialcl and cl.codcli=ct.codcli
inner join vdmodcontr mc on mc.codemp=ct.codempmc and mc.codfilial=ct.codfilialmc and mc.codmodcontr=ct.codmodcontr
left outer join vditcontrato ic on  ic.codemp=ct.codemp and ic.codfilial=ct.codfilial and ic.codcontr=ct.codcontr where ct.codemp=86 and ct.codfilial=1 and ct.codcontr=1 ]]>
