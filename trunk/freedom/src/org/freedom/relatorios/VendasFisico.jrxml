	<queryString><![CDATA[SELECT SUBSTRING(P.CODGRUP FROM 1 FOR 4) AS GRUPO,
P.CODPROD,P.REFPROD,P.DESCPROD,G.DESCGRUP,P.CUSTOMPMPROD,IT.CODITVENDA,
SUM(IT.QTDITVENDA) QTDITVENDA,
SUM(IT.VLRDESCITVENDA) VLRDESCITVENDA, 
SUM(IT.VLRLIQITVENDA) VLRLIQITVENDA 
FROM VDVENDA V,VDITVENDA IT, EQPRODUTO P,EQGRUPO G,EQTIPOMOV TM 
WHERE V.DTEMITVENDA BETWEEN '01.01.2006' AND '01.01.2007' 
AND V.CODEMP=IT.CODEMP AND V.CODFILIAL=IT.CODFILIAL AND V.CODVENDA=IT.CODVENDA AND V.TIPOVENDA=IT.TIPOVENDA
AND IT.CODEMP=4 AND IT.CODFILIAL=1
AND NOT SUBSTR(V.STATUSVENDA,1,1)='C' 
AND P.CODEMP=IT.CODEMPPD AND P.CODFILIAL=IT.CODFILIALPD AND P.CODPROD=IT.CODPROD
AND G.CODEMP=P.CODEMPGP AND G.CODFILIAL=P.CODFILIALGP AND G.CODGRUP=P.CODGRUP
AND TM.CODEMP=V.CODEMPTM AND TM.CODFILIAL=V.CODFILIALTM AND TM.CODTIPOMOV=V.CODTIPOMOV 
AND TM.FISCALTIPOMOV IN ('S','N') 
AND TM.SOMAVDTIPOMOV IN ('S','N') 
AND TM.TIPOMOV IN ('VD','VE','PV','VT','SE') 
AND (NOT IT.QTDITVENDA = 0) 
AND (V.FLAG IN ('S','N')) 
GROUP BY 1,P.CODPROD,P.REFPROD,P.DESCPROD,G.DESCGRUP,IT.CODITVENDA, P.CUSTOMPMPROD 
ORDER BY 1,P.DESCPROD]]></queryString>

