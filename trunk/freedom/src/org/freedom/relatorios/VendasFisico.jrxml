	<variable name="VLRTOTLUCRO" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{QTDITVENDA}.multiply( $V{VLRUNITLUCRO} )]]></variableExpression>
	</variable>
	<variable name="MARGEM" class="java.math.BigDecimal" resetType="None" calculation="Nothing">
