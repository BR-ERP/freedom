	<field name="QTDITVENDA" class="java.math.BigDecimal"/>
	<field name="VLRDESCITVENDA" class="java.math.BigDecimal"/>
	<field name="VLRLIQITVENDA" class="java.math.BigDecimal"/>
	<variable name="VLRUINTVENDA" class="java.math.BigDecimal" resetType="None">
		<variableExpression><![CDATA[$F{VLRLIQITVENDA}.divide( $F{QTDITVENDA}, 2, BigDecimal.ROUND_HALF_UP )]]></variableExpression>
	</variable>
	<variable name="VLRTOTCUSTO" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{CUSTOMPMPROD}.multiply( $F{QTDITVENDA} )]]></variableExpression>
	</variable>
	<variable name="VLRUNITLUCRO" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{VLRUINTVENDA}.subtract( $F{CUSTOMPMPROD} )]]></variableExpression>
	</variable>
	<variable name="VLRTOTLUCRO" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{QTDITVENDA}.multiply( $V{VLRUNITLUCRO} )]]></variableExpression>
	</variable>
	<variable name="MARGEM" class="java.math.BigDecimal" resetType="None">
		<variableExpression><![CDATA[//new BigDecimal( $V{VLRTOTLUCRO}.floatValue() / ( $F{VLRLIQITVENDA}.floatValue() / 100 ) )
$F{VLRLIQITVENDA}.floatValue() > 0 ?
new BigDecimal( $V{VLRTOTLUCRO}.floatValue() / ( $F{VLRLIQITVENDA}.floatValue() / 100 ) ) :
new BigDecimal(0)]]></variableExpression>
	</variable>
	<variable name="TOTGRUPOVENDA" class="java.math.BigDecimal" resetType="Group" resetGroup="GRUPO" calculation="Sum">
