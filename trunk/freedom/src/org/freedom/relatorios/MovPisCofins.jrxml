	<queryString>
		<![CDATA[select
    coalesce(pd2.codprod,pd1.codprod) codprod,
    coalesce(pd2.refprod,pd1.refprod) refprod,
    coalesce(pd2.descprod,pd1.descprod) descprod,
    coalesce(pd2.codunid,pd1.codunid) codunid,
    sum(case  when extract(month from v.dtemitvenda)=1 then iv.qtditvenda else 0 end) mes01,
    sum(case  when extract(month from v.dtemitvenda)=2 then iv.qtditvenda else 0 end) mes02,
    sum(case  when extract(month from v.dtemitvenda)=3 then iv.qtditvenda else 0 end) mes03,
    sum(case  when extract(month from v.dtemitvenda)=4 then iv.qtditvenda else 0 end) mes04,
    sum(case  when extract(month from v.dtemitvenda)=5 then iv.qtditvenda else 0 end) mes05,
    sum(case  when extract(month from v.dtemitvenda)=6 then iv.qtditvenda else 0 end) mes06,
    sum(case  when extract(month from v.dtemitvenda)=7 then iv.qtditvenda else 0 end) mes07,
    sum(case  when extract(month from v.dtemitvenda)=8 then iv.qtditvenda else 0 end) mes08,
    sum(case  when extract(month from v.dtemitvenda)=9 then iv.qtditvenda else 0 end) mes09,
    sum(case  when extract(month from v.dtemitvenda)=10 then iv.qtditvenda else 0 end) mes10,
    sum(case  when extract(month from v.dtemitvenda)=11 then iv.qtditvenda else 0 end) mes11,
    sum(case  when extract(month from v.dtemitvenda)=12 then iv.qtditvenda else 0 end) mes12,
    sum (iv.qtditvenda ) Total
from
    vdcliente c,
    eqtipomov tm,
    vdvenda v,
    vditvenda iv

left outer join eqproduto pd1 on pd1.codemp=iv.codemppd and pd1.codfilial=iv.codfilialpd and pd1.codprod=iv.codprod left outer join eqgrupo g1 on g1.codemp=pd1.codempgp and g1.codfilial=pd1.codfilialgp and g1.codgrup=pd1.codgrup
left outer join vdvendaorc vo on vo.codemp=iv.codemp and vo.codfilial=iv.codfilial and vo.codvenda=iv.codvenda and vo.tipovenda=iv.tipovenda and vo.coditvenda=iv.coditvenda
left outer join eqitrecmercitositorc iro on iro.codempoc=vo.codempor and iro.codfilialoc=vo.codfilialor and iro.codorc=vo.codorc and iro.tipoorc=vo.tipoorc and iro.coditorc=vo.coditorc
left outer join eqitrecmerc ir on ir.codemp=iro.codemp and ir.codfilial=iro.codfilial and ir.ticket=iro.ticket and ir.coditrecmerc=iro.coditrecmerc
left outer join eqproduto pd2 on pd2.codemp=ir.codemppd and pd2.codfilial=ir.codfilialpd and pd2.codprod=ir.codprod
left outer join eqgrupo g2 on g2.codemp=pd2.codempgp and g2.codfilial=pd2.codfilialgp and g2.codgrup=pd2.codgrup

WHERE
    iv.CODEMP=71 AND
    iv.CODFILIAL=1 AND
    V.CODEMP=Iv.CODEMP AND
    V.CODFILIAL=Iv.CODFILIAL AND
    V.CODVENDA=Iv.CODVENDA and
    v.tipovenda=iv.tipovenda AND
    V.CODCLI=C.CODCLI AND
    V.CODEMPCL=C.CODEMP AND
    V.CODFILIALCL=C.CODFILIAL AND
    V.DTEMITVENDA BETWEEN '01.01.2011' AND '31.12.2011' AND
    V.FLAG IN ('S','N')  AND
    V.CODCLI=113 AND
    TM.FISCALTIPOMOV='S' AND
    TM.SOMAVDTIPOMOV='S'  AND
    NOT SUBSTR(V.STATUSVENDA,1,1)='C' AND
    TM.CODEMP=V.CODEMPTM AND
    TM.CODFILIAL=V.CODFILIALTM AND
    TM.CODTIPOMOV=V.CODTIPOMOV AND
    TM.TIPOMOV IN ('VD','VE','PV','VT','SE')
GROUP BY
    1, 2, 3, 4
ORDER BY
    3]]>
	</queryString>
