	<queryString>
		<![CDATA[SELECT TM.CODTIPOMOV, TM.DESCTIPOMOV,
V.DOCVENDA, V.CODVENDA, V.DTEMITVENDA,
SUM( V.vlrliqvenda ) AS VALORLIQUIDO, C.RAZCLI
FROM FNPLANOPAG P, VDVENDA V, EQTIPOMOV TM, VDCLIENTE C
WHERE V.CODEMP=49 AND V.CODFILIAL=1 AND P.CODPLANOPAG=V.CODPLANOPAG AND
V.CODEMPPG=P.CODEMP AND V.CODFILIALPG=P.CODFILIAL AND
V.CODEMPTM=TM.CODEMP AND V.CODFILIALTM=TM.CODFILIAL AND V.CODTIPOMOV=TM.CODTIPOMOV AND
C.CODCLI = V.CODCLI AND V.VLRPRODVENDA > 0 AND
NOT SUBSTR(V.STATUSVENDA,1,1)='C'
GROUP BY TM.CODTIPOMOV, TM.DESCTIPOMOV, V.CODVENDA, V.DOCVENDA, V.DTEMITVENDA, C.RAZCLI]]>
	</queryString>
