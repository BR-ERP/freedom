	<queryString><![CDATA[
select PD.descprod, PD.vlrdensidade, PD.vlrconcent, UN.descunid,
(select sldprod from eqcustoprodsp(PD.CODEMP,PD.CODFILIAL,PD.codprod,
   '2005-10-31','P',PD.codempax,PD.codfilialax,PD.codalmox,'S') ) as saldoant,
(SELECT sum(EQ.qtdmovprod) FROM eqmovprod EQ, eqtipomov tm
WHERE EQ.codprod=1 AND EQ.dtmovprod BETWEEN '2005-11-01' AND '2005-11-30'
and tm.codemp=eq.codemptm and tm.codfilial=eq.codfilialtm and tm.codtipomov=eq.codtipomov
and tm.tipomov='RM') as utilizacao ,
(SELECT sum(EQ.qtdmovprod) FROM eqmovprod EQ, eqtipomov tm
WHERE EQ.codprod=1 AND EQ.dtmovprod BETWEEN '2005-11-01' AND '2005-11-30'
and tm.codemp=eq.codemptm and tm.codfilial=eq.codfilialtm and tm.codtipomov=eq.codtipomov
and tm.tipomov='CP') as compras,
(select sldprod from eqcustoprodsp(PD.CODEMP,PD.CODFILIAL,PD.codprod,
   '2005-12-1','P',PD.codempax,PD.codfilialax,PD.codalmox,'S') ) as saldoatual
FROM eqproduto PD, equnidade UN
WHERE UN.codunid=PD.codunid
AND PD.CODEMP=7 AND PD.CODFILIAL=1 AND PD.codprod=1]]></queryString>

