(case when (cast('today' as date) - (PP.DTALT))<7 then 'S' else 'N' end) as alterado,P.SLDPROD
FROM SGPREFERE1 P1, EQPRODUTO P, VDPRECOPROD PP
WHERE P.CODEMP=PP.CODEMP AND P.CODFILIAL=PP.CODFILIAL AND P.CODPROD=PP.CODPROD
AND PP.CODEMP=49 AND PP.CODFILIAL=1 AND PP.CODPLANOPAG=1
