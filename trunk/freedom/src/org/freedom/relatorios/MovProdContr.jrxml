	<parameter name="DATAFIM" isForPrompting="false" class="java.util.Date"/>
	<parameter name="CODEMP" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="CODFILIAL" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="CODPROD" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[select PD.descprod, PD.vlrdensidade, PD.vlrconcent, UN.descunid,
(select sldprod from eqcustoprodsp(PD.CODEMP,PD.CODFILIAL,PD.codprod,
   $P{DATAFIM},'P',PD.codempax,PD.codfilialax,PD.codalmox,'S') ) as saldoant,
(SELECT sum(EQ.qtdmovprod) FROM eqmovprod EQ, eqtipomov tm
WHERE EQ.codprod=11 AND EQ.dtmovprod BETWEEN $P{DATAINI} AND $P{DATAFIM}
and tm.codemp=eq.codemptm and tm.codfilial=eq.codfilialtm and tm.codtipomov=eq.codtipomov
and tm.tipomov='RM') as utilizacao ,
(SELECT sum(EQ.qtdmovprod) FROM eqmovprod EQ, eqtipomov tm
WHERE EQ.codprod=11 AND EQ.dtmovprod BETWEEN $P{DATAINI} AND $P{DATAFIM}
and tm.codemp=eq.codemptm and tm.codfilial=eq.codfilialtm and tm.codtipomov=eq.codtipomov
and tm.tipomov='CP') as compras,
(select sldprod from eqcustoprodsp(PD.CODEMP,PD.CODFILIAL,PD.codprod,
   $P{DATAFIM},'P',PD.codempax,PD.codfilialax,PD.codalmox,'S') ) as saldoatual
FROM eqproduto PD, equnidade UN
WHERE UN.codunid=PD.codunid
AND PD.CODEMP=$P{CODEMP} AND PD.CODFILIAL=$P{CODFILIAL} AND PD.codprod=$P{CODPROD}]]></queryString>

